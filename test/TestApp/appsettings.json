{
    "AllowedHosts": "*",
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "Seo": {
        "RobotsTxt": {
            "AddSitemapUrl": false,
            "UserAgentRecords": [
                {
                    "UserAgent": "bing",
                    "Disallow": ["/login"]
                },
                {
                    "UserAgent": "google",
                    "Disallow": ["*"]
                }
            ]
        },
        "Sitemap": {
            "Urls": [
                {
                    "Priority": 0.9,
                    "RelativeUrl": "/login"
                },
                {
                    "Priority": 0.8,
                    "RelativeUrl": "/about"
                }
            ]
        }
    }
}